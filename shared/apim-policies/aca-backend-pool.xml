<!--
    This policy sample calls an existing Azure Container Apps backend.
-->
<policies>
    <inbound>
        <base />
        <set-backend-service backend-id="aca-backend-pool" />
    </inbound>
    <backend>
        <base />
    </backend>
    <outbound>
        <base />
    </outbound>
    <on-error>
        <base />
    </on-error>
</policies>
